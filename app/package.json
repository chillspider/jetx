{
	"name": "mobile",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare-env": "bash scripts/prepare-env.sh",
		"android": "ENVFILE=.env.prod react-native run-android",
		"android:dev": "ENVFILE=.env.dev react-native run-android",
		"ios": "ENVFILE=.env.prod react-native run-ios --simulator=\"iPhone 15 Pro Max\" ",
		"ios:dev": "ENVFILE=.env.dev react-native run-ios --simulator \"iPhone 15 Pro Max\"",
		"lint": "eslint .",
		"start": "react-native start",
		"test": "jest",
		"type-check": "tsc",
		"test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
		"pod-install": "cd ios && pod install && cd ..",
		"postinstall": "npx patch-package",
		"build:android": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew bundleRelease",
		"build:android:dev": "cd android && ./gradlew clean && ENVFILE=.env.dev ./gradlew assembleRelease",
		"build:androidApk": "cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew assembleRelease",
		"build:ios": "cd ios && pod install && fastlane release",
		"build:ios:dev": "cd ios && pod install && fastlane dev",
		"appcenter:androidDev": "bash scripts/build-android.sh dev",
		"appcenter:androidProd": "bash scripts/build-android.sh prod",
		"appcenter:iosDev": "bash scripts/build-ios.sh dev",
		"appcenter:iosProd": "bash scripts/build-ios.sh prod",
		"codepush:android": "appcenter codepush release-react -a Wash24hOrg/JetX-Android -d Production -t \"1.1.*\"",
		"codepush:ios": "appcenter codepush release-react -a Wash24hOrg/JetX-IOS -d Production --plist-file ios/mobile/Info.plist -t \"1.1.*\""
	},
